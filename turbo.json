{
  "$schema": "https://turborepo.org/schema.json",
  "globalDotEnv": [
    ".env"
  ],
  "pipeline": {
    "dev": {
      "cache": false,
      "dotEnv": [
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ]
    },
    "generate": {},
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".cdk.out/**"
      ]
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "./out/**"
      ]
    },
    "synth": {
      "outputs": [
        "./cdk.out/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
