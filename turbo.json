{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "BLOG_ENDPOINT",
    "BLOG_TOKEN"
  ],
  "pipeline": {
    "//#codegen": {
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "build:open": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        ".open-next/**"
      ]
    },
    "synth": {
      "outputs": [
        "./cdk.out/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
